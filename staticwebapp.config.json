{
  "routes": [
    {
      "route": "/about.html",
      "rewrite": "/about.html"
    },
    {
      "route": "/services.html",
      "rewrite": "/services.html"
    },
    {
      "route": "/contact.html",
      "rewrite": "/contact.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/about.html",
      "/services.html",
      "/contact.html"
    ]
  }
}
{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/css/*",
      "/js/*",
      "/partials/*",
      "/sitemap.xml"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}
